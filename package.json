{
  "name": "rollup-plugin-html-bundler",
  "version": "1.4.2",
  "main": "lib/bundler-node.js",
  "repository": "https://github.com/VandeurenGlenn/html-bundler.git",
  "author": "Glenn Vandeuren <vandeurenglenn@gmail.com>",
  "license": "MIT",
  "scripts": {
    "deploy": "npm run build && yarn upgrade && npm publish",
    "build": "backed -b",
    "test": "mocha"
  },
  "files": [
    "lib"
  ],
  "backed": {
    "bundles": [
      {
        "src": "src/index.js",
        "dest": "lib/bundler.js",
        "format": "es",
        "plugins": {
          "babel": {
            "babelrc": false,
            "presets": [
              "es2016"
            ],
            "plugins": [
              "external-helpers",
              "transform-async-generator-functions"
            ]
          }
        }
      },
      {
        "src": "src/index.js",
        "dest": "lib/bundler.js",
        "format": "cjs",
        "plugins": {
          "babel": {
            "babelrc": false,
            "presets": [
              [
                "es2015",
                {
                  "modules": false
                }
              ]
            ],
            "plugins": [
              "transform-async-generator-functions",
              "external-helpers",
              [
                "transform-runtime",
                {
                  "polyfill": false,
                  "regenerator": true
                }
              ]
            ]
          }
        }
      }
    ],
    "sources": [
      {
        "src": [
          "node_modules/babel-runtime/helpers/{classCallCheck,createClass,regenerator,asyncGenerator}*.js",
          "node_modules/babel-runtime/regenerator/*.js"
        ],
        "dest": "lib/node_modules"
      }
    ]
  },
  "dependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "backed-bundler": "^0.1.0",
    "backed-splitter": "^1.4.0",
    "dom5": "^2.2.0",
    "parse5": "^3.0.2",
    "rollup-pluginutils": "^2.0.1"
  },
  "devDependencies": {
    "mocha": "^3.2.0",
    "rollup": "^0.41.6"
  }
}
